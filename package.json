{
  "name": "@schibstedspain/zeus-api-generator",
  "version": "0.0.1",
  "description": "Generate static API from HTML files",
  "main": "index.js",
  "scripts": {
    "prepublish": "rimraf ./lib && npm run dist",
    "dist": "npm run dist:js",
    "dist:js": "babel src --out-dir lib",
    "predev": "rimraf docs/bundle.js && touch docs/bundle.js",
    "dev": "npm run dev:js:watch & npm run dev:server",
    "dev:js:watch": "watchify -t babelify -t sassify docs/index.js -o docs/bundle.js",
    "dev:server": "serve ./docs -p 8080",
    "test": "mocha --compilers js:babel/register --recursive",
    "test:watch": "npm run test -- --watch",
    "lint": "npm run lint:jscs && npm run lint:eslint",
    "lint:jscs": "cat ./{src,test}/**/*.js | jscs",
    "lint:eslint": "eslint --ext=\".js\" src/",
    "predoc": "browserify -t babelify docs/index.js -o docs/bundle.js",
    "doc": "npm run doc:build",
    "doc:build": "gh-pages -d docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zeus-Project/zeus-api-generator.git"
  },
  "keywords": [
    "api",
    "generator",
    "static",
    "github pages"
  ],
  "author": "Carlos Villuendas <carlosvillu@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Zeus-Project/zeus-api-generator/issues"
  },
  "pre-commit": [
    "test"
  ],
  "devDependencies": {
    "@schibstedspain/frontend-pre-commit-rules": "latest",
    "babel": "^5.6.14",
    "babel-eslint": "~3.1.14",
    "babel-jscs": "^1.0.1",
    "babelify": "^6.1.3",
    "browserify": "^11.0.0",
    "chai": "^2.3.0",
    "eslint": "^0.24.1",
    "eslint-plugin-react": "^2.7.0",
    "gh-pages": "^0.3.1",
    "jscs": "^1.13.1",
    "mocha": "^2.2.4",
    "rimraf": "^2.4.2",
    "sassify": "^0.9.1",
    "serve": "^1.4.0",
    "sinon": "^1.15.4",
    "watchify": "^3.3.0"
  },
  "homepage": "https://github.com/Zeus-Project/zeus-api-generator#readme",
  "dependencies": {
    "cheerio": "^0.19.0"
  }
}
